disable_abort_on_error;

use udf_infusion_test;

select percentile_discr();
#   ERROR HY000: Can't initialize function 'percentile_discr'; percentile_discr must have exactly two arguments

select percentile_discr(1, 2, 3);
#   ERROR HY000: Can't initialize function 'percentile_discr'; percentile_discr must have exactly two arguments

select percentile_discr(NULL, 0.5);
#   percentile_discr(NULL, 0.5)
#   NULL

select percentile_discr(x, 0.5) from empty_table;
#   percentile_discr(x, 0.5)
#   NULL

select percentile_discr(x, 0.5) from null_table;
#   percentile_discr(x, 0.5)
#   NULL

select percentile_discr(x, 0.5) from small_table;
#   percentile_discr(x, 0.5)
#   4

select percentile_discr(x, 0) from small_table;
#   percentile_discr(x, 0)
#   1

select percentile_discr(x, 1) from small_table;
#   percentile_discr(x, 1)
#   8

select percentile_discr(x, 0.001) from small_table;
#   percentile_discr(x, 0.001)
#   1

select percentile_discr(x, 0.999) from small_table;
#   percentile_discr(x, 0.999)
#   8

select percentile_discr(x, 0.501) from small_table;
#   percentile_discr(x, 0.501)
#   5

select g, percentile_discr(x, 0.7) from small_table group by g;
#   g	percentile_discr(x, 0.7)
#   1	2
#   2	5
#   3	8
